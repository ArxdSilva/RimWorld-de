<?xml version="1.0" encoding="utf-8" ?>
<LanguageData>
	
  <!-- PawnAndItem -->
	
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[0]</path>
    <trans>image->[pawn_nameFull] [circumstance_phrase] beim Erstellen von [thing_labelIndefinite]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[1]</path>
    <trans>image->[pawn_nameFull] [circumstance_phrase] beim Beenden [pawn_possessive]er Arbeit an [item]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[2]</path>
    <trans>image->[pawn_nameFull] [circumstance_phrase], wie [pawn_pronoun] an [item] arbeitet. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[3]</path>
    <trans>circumstance_phrase->voll konzentriert</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[4]</path>
    <trans>circumstance_phrase->mit einem Ausdruck der Zufriedenheit auf [pawn_possessive]em Gesicht</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[5]</path>
    <trans>circumstance_phrase->an [pawn_possessive]em Arbeitsplatz</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[6]</path>
    <trans>extra_sentence->[thing_labelDefinite] scheint von [thing_quality] Qualität zu sein.</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rulesStrings[7]</path>
    <trans>extra_sentence->Unten drunter steht der von [pawn_nameShortDefinite] gewählte Titel: [thing_title].</trans>
  </rep>
  
	
  <rep>
    <path>StruckMineable.rulePack.rulesStrings[0]</path>
    <trans>image->[pawn_nameFull], wie [pawn_pronoun] [circumstance_phrase] auf [thing_label] trifft. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rulesStrings[1]</path>
    <trans>circumstance_phrase->während des Abbauens</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rulesStrings[2]</path>
    <trans>extra_sentence-> [pawn_nameShortDefinite] wischt sich den Schweiß von der Stirn.</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rulesStrings[3]</path>
    <trans>extra_sentence-> [pawn_nameShortDefinite] springt vor Freude in die Luft.</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rulesStrings[4]</path>
    <trans>extra_sentence-> Eine Gruppe Zuschauer hat sich um [pawn_nameShortDefinite] gescharrt.</trans>
  </rep>
  
	
  <!-- Special -->
	
  <rep>
    <path>KilledBy.rulePack.rulesStrings[0]</path>
    <trans>image->[victim_nameFull], wie [victim_pronoun] [circumstance_phrase] von [killer_nameFull] getötet wurde.</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rulesStrings[1]</path>
