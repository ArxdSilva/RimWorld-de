<?xml version="1.0" encoding="utf-8" ?>
<LanguageData>
	
  <!-- PawnAndItem -->
	
  <rep>
    <path>CraftedArt.rulePack.rules[0]</path>
    <trans>image->[pawn_nameFull] erstellt [thing_labelIndefinite] [circumstance_phrase]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[1]</path>
    <trans>image->[pawn_nameFull] beendet [pawn_possessive]e Arbeit an [item] [circumstance_phrase]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[2]</path>
    <trans>image->[pawn_nameFull] arbeitet an [item] [circumstance_phrase]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[3]</path>
    <trans>circumstance_phrase->voll konzentriert</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[4]</path>
    <trans>circumstance_phrase->mit einem Ausdruck der Zufriedenheit auf [pawn_possessive]em Gesicht</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[5]</path>
    <trans>circumstance_phrase->an [pawn_possessive]em Arbeitsplatz</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[6]</path>
    <trans>extra_sentence-> [thing_labelDefinite] scheint von [thing_quality] Qualität zu sein.</trans>
  </rep>
  <rep>
    <path>CraftedArt.rulePack.rules[7]</path>
    <trans>extra_sentence-> Unten drunter steht der von [pawn_nameShortDefinite] gewählte Titel: [thing_title].</trans>
  </rep>
  
	
  <rep>
    <path>StruckMineable.rulePack.rules[0]</path>
    <trans>image->[pawn_nameFull] schlägt [thing_label] [circumstance_phrase]. [extra_sentence]</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rules[1]</path>
    <trans>circumstance_phrase->beim Abbauen</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rules[2]</path>
    <trans>extra_sentence-> [pawn_nameShortDefinite] wischt sich den Schweiß von der Stirn.</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rules[3]</path>
    <trans>extra_sentence-> [pawn_nameShortDefinite] springt vor Freude in die Luft.</trans>
  </rep>
  <rep>
    <path>StruckMineable.rulePack.rules[4]</path>
    <trans>extra_sentence-> Eine Gruppe Zuschauer hat sich um [pawn_nameShortDefinite] gescharrt.</trans>
  </rep>
  
	
  <!-- Special -->
	
  <rep>
    <path>KilledBy.rulePack.rules[0]</path>
    <trans>image->[victim_nameFull] wurde [circumstance_phrase] von [killer_nameFull] getötet.</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[1]</path>
    <trans>image->[killer_nameFull] tötet [circumstance_phrase] [victim_nameFull].</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[2]</path>
    <trans>image->[killer_nameFull] beendet [circumstance_phrase] das Leben von [victim_nameFull].</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[3]</path>
    <trans>image->[killer_nameFull] ermordet [victim_nameFull] [circumstance_phrase].</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[4]</path>
    <trans>image->[victim_nameFull] wird [circumstance_phrase] getötet.</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[5]</path>
    <trans>circumstance_phrase->scheinbar ohne Reue</trans>
  </rep>
  <rep>
    <path>KilledBy.rulePack.rules[6]</path>
    <trans>circumstance_phrase->mit eiskalter Präzision</trans>
  </rep>
  
	
</LanguageData>